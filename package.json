{
  "name": "RETO_3",
  "version": "1.0.0",
  "scripts": {
    "test:ui": "npx cucumber-js test/features --require-module ts-node/register --require test/steps/**/*.ts --format json:reports/cucumber_report.json",
    "report:html": "node generate-report.js",
    "test:api": "jest --config jest.config.js",
    "test:all": "npm run test:ui && npm run report:html && npm run test:api",
    "open:reports": "xdg-open reports/cucumber_report.html && xdg-open reports/api/index.html"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "@playwright/test": "^1.48.0",
    "@swc/core": "^1.15.1",
    "@swc/helpers": "^0.5.17",
    "@types/node": "^24.10.0",
    "cucumber-html-reporter": "^7.2.0",
    "jest": "^30.2.0",
    "jest-stare": "^2.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "node-fetch": "^2.6.12"
  }
}



